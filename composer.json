{
    "name": "jadu/meteor",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jadu/doctrine-types.git"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "~6.0",
        "symfony/config": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/process": "^5.4",
        "symfony/options-resolver": "^5.4",
        "symfony/yaml": "^5.4",
        "doctrine/dbal": "^2.9",
        "doctrine/migrations": "^2.3.5",
        "jadu/doctrine-types": "v2.x-dev as 2.0.0",
        "composer/semver": "^3.2",
        "composer/package-versions-deprecated": "1.11.99.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "mikey179/vfsstream": "*",
        "humbug/box": "^3.16",
        "friendsofphp/php-cs-fixer": "^2.0",
        "jadu/php-style": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "Meteor\\": "src/",
            "Jadu\\Meteor\\": "src/BackwardsCompatibility"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meteor\\": "tests/"
        }
    },
    "config": {
        "stability": "dev",
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "ocramius/package-versions": true
        }
    },
    "bin": ["bin/meteor"]
}
